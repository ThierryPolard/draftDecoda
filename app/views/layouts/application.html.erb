<!DOCTYPE html>
<html>
  <head>
    <!-- <meta charset="utf-8"> -->
    <title>Decoda</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <!-- Basic Global javascript and css-->
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "main" %>
    <%= stylesheet_link_tag "theme.min" %>

    <!--<link rel='stylesheet' id='pe_theme_compressed-css'  href='http://weareants.fr/wp-content/themes/visia/theme/compressed/theme.min.css?ver=1444283240' type='text/css' media='all' /> -->
    <!--<link rel='stylesheet' id='pe_theme_init-css'  href='http://weareants.fr/wp-content/themes/visia/style.css?ver=1444283240' type='text/css' media='all' />-->

     <!-- leaflet css and java script -->
    <%= stylesheet_link_tag href="//cdn.leafletjs.com/leaflet-0.7.2/leaflet.css" , media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "//cdn.leafletjs.com/leaflet-0.7.2/leaflet.js" , "data-turbolinks-track" => true %>
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" media="screen" rel="stylesheet" />

    <!-- fontmarker and integration to leaflet -->
    <%= stylesheet_link_tag href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css"  %>
    <link rel="stylesheet" href="dist/leaflet.awesome-markers.css"/>
    <link href='//fonts.googleapis.com/css?family=Roboto:400,300,500,700' rel='stylesheet' type='text/css'/>
    <script src="dist/leaflet.awesome-markers.js"></script>
    <script href="/application-11e91eca62504b11c24a0e36acbb5534.css"></script>

    <!-- integrating google map and leaflet -->
    <%= javascript_include_tag "//matchingnotes.com/javascripts/leaflet-google.js", "data-turbolinks-track" => true %>

    <!--<%= javascript_include_tag "//maps.google.com/maps/api/js?v=3.2&sensor=false", "data-turbolinks-track" => true %>-->
    <script src="http://maps.google.com/maps/api/js?v=3.23"></script>

    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,400,700"/>
    <!-- <%= csrf_meta_tags %> -->

    <!-- head pour le graph -->
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>

    <!-- favicon -->
    <link rel="shortcut icon" href="http://veille-eau.com/favicon.ico" />

</head>


<body id="<%= params[:controller]+"-"+params[:action] %>">


  <!--<section>
  <%= render partial: "/layouts/header" %>
  </section>-->

  <section>
  <%= render partial: "/layouts/navigation" %>
  </section>

   <section>
  <%= render partial: "/layouts/intro" %>
  </section>

  <section>
      <%= render partial: "/layouts/enSavoirPlus" %>
  </section>

  <section class = "carte" >
      <%= render partial: "/layouts/carte" %>
  </section>

  <section>
    <div class ="graph">
    <%= render partial: "/layouts/graph" %>
    </div>
  </section>


  <section>
    <%= render partial: "/layouts/accessData" %>
    <%= yield %>
  </section>

  <section>
    <%= render partial: "/layouts/partenaires" %>
  </section>

  <section>
    <%= render partial: "/layouts/contact" %>
  </section>

  <!--
  <section>
    <%= render partial: "/layouts/footer" %>
  </section>
  -->
</body>

</html>
